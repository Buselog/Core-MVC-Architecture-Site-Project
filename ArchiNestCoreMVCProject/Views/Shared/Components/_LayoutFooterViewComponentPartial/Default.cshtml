@model List<ArchiNestCoreMVCProject.Entities.Field>

<footer id="footer" class="footer dark-background">

	<div class="container footer-top">
		<div class="row gy-4">
			<div class="col-lg-4 col-md-6 footer-about">
				<a href="index.html" class="logo d-flex align-items-center">
					<span class="sitename">ArchiNest</span>
				</a>
				<div class="footer-contact pt-3">
					<p>@ViewBag.Address</p>
				
					<p class="mt-3"><strong>Phone:</strong> <span>@ViewBag.Phone</span></p>
					<p><strong>Email:</strong> <span>@ViewBag.Email</span></p>
				</div>
				<div class="social-links d-flex mt-4">
					<a href=""><i class="bi bi-twitter-x"></i></a>
					<a href=""><i class="bi bi-facebook"></i></a>
					<a href=""><i class="bi bi-instagram"></i></a>
					<a href=""><i class="bi bi-linkedin"></i></a>
				</div>
			</div>

			<div class="col-lg-2 col-md-3 footer-links">
				<h4>Useful Links</h4>
				<ul>
					<li><i class="bi bi-chevron-right"></i> <a href="#about">About</a></li>
					<li><i class="bi bi-chevron-right"></i> <a href="#services">Services</a></li>
					<li><i class="bi bi-chevron-right"></i> <a href="#portfolio">Portfolio</a></li>
					<li><i class="bi bi-chevron-right"></i> <a href="#testimonial">Testimonial</a></li>
					<li><i class="bi bi-chevron-right"></i> <a href="#team">Team Member</a></li>
				</ul>
			</div>

			<div class="col-lg-2 col-md-3 footer-links">
				<h4>Our Top 5 Services</h4>
				<ul>
					@foreach(var item in Model){
						<li><i class="bi bi-chevron-right"></i> <a href="#">@item.FieldName</a></li>
					}
					
				</ul>
			</div>

			<div class="col-lg-4 col-md-12 footer-newsletter">
				<h4>Our Newsletter</h4>
				<p>Subscribe to our newsletter and receive the latest news about our products and services!</p>
				<form action="forms/newsletter.php" method="post" class="php-email-form">
					<div class="newsletter-form"><input type="email" name="email"><input type="submit" value="Subscribe"></div>
					<div class="loading">Loading</div>
					<div class="error-message"></div>
					<div class="sent-message">Your subscription request has been sent. Thank you!</div>
				</form>
			</div>

		</div>
	</div>

	<div class="container copyright text-center mt-4">
		<p>© <span>Copyright</span> <strong class="px-1 sitename">ArchiNest</strong> <span>All Rights Reserved</span></p>
	</div>

</footer>