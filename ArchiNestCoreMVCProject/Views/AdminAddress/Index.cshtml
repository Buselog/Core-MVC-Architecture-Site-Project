@model ArchiNestCoreMVCProject.Entities.Address
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="col-lg-12 grid-margin stretch-card">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Services</h4>
			<div class="table-responsive">
				<table class="table table-striped table-hover">
					<tr>
						<th>Full Address</th>
						<td>@Model.FullAddress</td>
					</tr>
					<tr>
						<th>Phone</th>
						<td>@Model.Phone</td>
					</tr>
					<tr>
						<th>Email</th>
						<td>@Model.Email</td>
					</tr>
					<tr>
						<th>Operations</th>
						<td>
							<a type="button" class="btn btn-inverse-primary btn-fw" href="/AdminAddress/Update/@Model.AddressId">Update</a>
							&nbsp;
							<a type="button" class="btn btn-inverse-danger btn-fw" href="/AdminAddress/Delete/@Model.AddressId">Delete</a>
						</td>
					</tr>

				</table>
			</div>
		</div>
	</div>
</div>

