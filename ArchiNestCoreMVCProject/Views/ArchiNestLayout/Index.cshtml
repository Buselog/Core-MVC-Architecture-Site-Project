@{
	Layout = null;
}

@* <!DOCTYPE html>
<html lang="en"> *@

@await Component.InvokeAsync("_LayoutHeadViewComponentPartial")

<body class="index-page">

	@await Component.InvokeAsync("_LayoutHeaderViewComponentPartial")

	<main class="main">

		<!-- Hero Section -->
		@await Component.InvokeAsync("_LayoutHomeViewComponentPartial")

		<!-- /Hero Section -->
		<!-- About Section -->
		@await Component.InvokeAsync("_LayoutAboutViewComponentPartial")

		<!-- /About Section -->
		<!-- Stats Section -->
		@await Component.InvokeAsync("_LayoutStatisticViewComponentPartial")

		<!-- /Stats Section -->
		<!-- Services Section -->
		@await Component.InvokeAsync("_LayoutServicesViewComponentPartial")
		<!-- /Services Section -->
		<!-- Features Section -->
		@await Component.InvokeAsync("_LayoutPortfolioViewComponentPartial")
		<!-- /Features Section -->
		<!-- Testimonials Section -->
		@await Component.InvokeAsync("_LayoutTestimonialViewComponentPartial")
		<!-- /Testimonials Section -->
		<!-- Team Section -->
		@await Component.InvokeAsync("_LayoutTeamViewComponentPartial")
		<!-- /Team Section -->
		<!-- /Team Section -->
		<!-- Contact Section -->
		<section id="contact" class="contact section">

			<!-- Section Title -->
			<div class="container section-title" data-aos="fade-up">
				<h2>Contact</h2>
				<p>Contact Us</p>
			</div><!-- End Section Title -->

			<div class="container" data-aos="fade-up" data-aos-delay="100">

				<div class="row gy-4">
					@await Component.InvokeAsync("_LayoutAddressViewComponentPartial")

					@await Component.InvokeAsync("_LayoutContactViewComponentPartial")

				</div>

			</div>

		</section>
		<!-- /Contact Section -->

	</main>

	@await Component.InvokeAsync("_LayoutFooterViewComponentPartial")

	<!-- Scroll Top -->
	<a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

	<!-- Preloader -->
	<div id="preloader"></div>

	<!-- Vendor JS Files -->
	<script src="/ArchiNest/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
	<script src="/ArchiNest/assets/vendor/php-email-form/validate.js"></script>
	<script src="/ArchiNest/assets/vendor/aos/aos.js"></script>
	<script src="/ArchiNest/assets/vendor/glightbox/js/glightbox.min.js"></script>
	<script src="/ArchiNest/assets/vendor/purecounter/purecounter_vanilla.js"></script>
	<script src="/ArchiNest/assets/vendor/swiper/swiper-bundle.min.js"></script>
	<script src="/ArchiNest/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
	<script src="/ArchiNest/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>

	<!-- Main JS File -->
	<script src="/ArchiNest/assets/js/main.js"></script>

</body>

</html>
